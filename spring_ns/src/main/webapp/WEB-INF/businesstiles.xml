<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" 
"http://tiles.apache.org/dtds/tiles-config_3_0.dtd">

<tiles-definitions>
	<definition name=".business"  template="/WEB-INF/views/business/bnlayout.jsp" >
		<put-attribute name="head" value="/WEB-INF/views/business/bnheader.jsp"/>
		<put-attribute name="con" value="/WEB-INF/views/business/bnMain.jsp"/>
	</definition>
	
	<definition name=".business.*.*.*"  extends=".business">
		<put-attribute name="con" value="/WEB-INF/views/admin/{1}/{2}/{3}.jsp"/>
	</definition>
	
	<definition name=".business.*.*"  extends=".business">
		<put-attribute name="con" value="/WEB-INF/views/admin/{1}/{2}.jsp"/>
	</definition>
	
	<definition name=".business.*"  extends=".business">
		<put-attribute name="con" value="/WEB-INF/views/admin/{1}.jsp"/>
	</definition>
	
	
	
	

</tiles-definitions>